(in-package :cl-fourier-animation)

(defstruct shape coords size scale offset fft freq-idx-transform-fn fft-idx-sorted)

(defun path->shape (path &key (offset (complex 0 0)) (scale 1500) (points-per-segment 100) (fft-size 512))
  "transform a svg path into a shape struct. The parser just extracts
the points of the path and doesn't interpolate between points. The
number of points on the path needs to be a power of two."
  (let* ((coords (normalize-coords (get-coords path fft-size points-per-segment))))
    (unless (power-of-two? fft-size)
      (error "path contains non powoer of two number of points: ~a" (length coords)))
    (let* ((complex-coords (mapcar #'vector->complex coords))
           (fft (normalize-fft
                 (bordeaux-fft:fft
                  (coerce (make-array fft-size :initial-contents complex-coords)
                   'bordeaux-fft:complex-sample-array)))))
      (make-shape
       :coords complex-coords
       :size fft-size
       :scale scale
       :offset offset
       :fft fft
       :freq-idx-transform-fn (get-transform-fn fft-size)
       :fft-idx-sorted
       (coerce
        (mapcar #'first
                (sort (loop for i from 0 for x across fft collect (list i x))
                      #'>
                      :key (lambda (x) (abs (second x)))))
        'vector)))))

(defparameter *arrow-path*
  (make-path 0.0 0.015 0.85 0.015 0.85 0.1 1.0 0.0 0.85 -0.1 0.85 -0.015 0.0 -0.015
             0.0 0.015))



